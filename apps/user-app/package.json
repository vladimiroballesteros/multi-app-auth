{
  "name": "user-app",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node server/server.js",
    "dev:client": "vite",
    "build": "vite build && npm run build:server",
    "build:server": "cp -r server dist/ && cp package.json dist/",
    "preview": "vite preview",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "express": "^4.18.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^4.4.0",
    "concurrently": "^8.2.0"
  }
}